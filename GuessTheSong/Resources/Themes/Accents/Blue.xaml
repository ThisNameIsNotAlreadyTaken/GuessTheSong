<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="AccentColor">#006ac1</Color>
    <Color x:Key="RedColor">#EB0C43</Color>
    <Color x:Key="GreenColor">MediumSeaGreen</Color>
    <Color x:Key="LightGreenColor">LimeGreen</Color>
    <Color x:Key="BlackColor">#202124</Color>
    <Color x:Key="WhiteColor">White</Color>
    <Color x:Key="SnowColor">Snow</Color>
    <Color x:Key="LightGrayColor">LightGray</Color>
    <Color x:Key="SilverColor">Silver</Color>
    <Color x:Key="GoldColor">Gold</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}"/>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource GreenColor}"/>
    <SolidColorBrush x:Key="LightGreenBrush" Color="{StaticResource LightGreenColor}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource BlackColor}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="SnowBrush" Color="{StaticResource SnowColor}"/>
    <SolidColorBrush x:Key="LightGrayBrush" Color="{StaticResource LightGrayColor}"/>
    <SolidColorBrush x:Key="SilverBrush" Color="{StaticResource SilverColor}"/>
    <SolidColorBrush x:Key="GoldBrush" Color="{StaticResource GoldColor}"/>
</ResourceDictionary>